<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yuqiaotech.helloj2ee.hibernate.sample1.Address" table="address">
		<id name="aid">
			<generator class="uuid.hex"/>
		</id>
		<property name="name"/>
		<property name="zipcode"/>
		<many-to-one name="person" class="com.yuqiaotech.helloj2ee.hibernate.sample1.Person">
		  <column name="pid"/>
		</many-to-one>
	</class>
</hibernate-mapping>