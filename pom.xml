     <build>
          <!-- 默认源代码和资源文件目录配置 -->
          <sourceDirectory>src/main/java</sourceDirectory>
          <testSourceDirectory>src/test/java</testSourceDirectory>
          <resources>
               <resource>
                    <directory>src/main/resources</directory>
               </resource>
          </resources>
          <testResources>
               <testResource>
                    <directory>src/test/resources</directory>
               </testResource>
          </testResources>
          <plugins>
               <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>3.6.0</version>
                    <configuration>
                         <source>1.8</source>
                         <target>1.8</target>
                         <!-- <excludes> <exclude>**/edu/jiangxin/webservice/**</exclude>
                              </excludes> -->
                    </configuration>
               </plugin>
               <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>build-helper-maven-plugin</artifactId>
                    <version>1.12</version>
                    <executions>
                         <execution>
                              <id>add-source</id>
                              <phase>generate-sources</phase>
                              <goals>
                                   <goal>add-source</goal>
                              </goals>
                              <configuration>
                                   <sources>
                                        <source>${basedir}/designpatternsrc/main/java</source>
                                        <source>${basedir}/encryptsrc/main/java</source>
                                        <source>${basedir}/shengsiyuansrc/main/java</source>
                                   </sources>
                              </configuration>
                         </execution>
                         <execution>
                              <id>add-resource</id>
                              <phase>generate-resources</phase>
                              <goals>
                                   <goal>add-resource</goal>
                              </goals>
                              <configuration>
                                   <resources>
                                        <resource>
                                             <directory>${basedir}/designpatternsrc/main/resources</directory>
                                             <directory>${basedir}/encryptsrc/main/resources</directory>
                                             <directory>${basedir}/shengsiyuansrc/main/resources</directory>
                                        </resource>
                                   </resources>
                              </configuration>
                         </execution>
                         <execution>
                              <id>add-test-sources</id>
                              <phase>generate-test-sources</phase>
                              <goals>
                                   <goal>add-test-source</goal>
                              </goals>
                              <configuration>
                                   <sources>
                                        <source>${basedir}/shengsiyuansrc/test/java</source>
                                   </sources>
                              </configuration>
                         </execution>
                         <execution>
                              <id>add-test-resources</id>
                              <phase>generate-test-resources</phase>
                              <goals>
                                   <goal>add-test-resource</goal>
                              </goals>
                              <configuration>
                                   <resources>
                                        <resource>
                                             <directory>${basedir}/shengsiyuansrc/test/resources</directory>
                                        </resource>
                                   </resources>
                              </configuration>
                         </execution>
                    </executions>
               </plugin>
               <plugin>
                    <groupId>org.antlr</groupId>
                    <artifactId>antlr4-maven-plugin</artifactId>
                    <version>4.5.3</version>
                    <executions>
                         <execution>
                              <id>antlr</id>
                              <goals>
                                   <goal>antlr4</goal>
                              </goals>
                         </execution>
                    </executions>
               </plugin>
               <plugin>
                    <groupId>org.jvnet.jax-ws-commons</groupId>
                    <artifactId>jaxws-maven-plugin</artifactId>
                    <version>2.3</version>
                    <executions>
                         <execution>
                              <goals>
                                   <goal>wsimport</goal>
                              </goals>
                              <configuration>
                                   <args>
                                        <arg>-encoding</arg>
                                        <arg>UTF-8</arg>
                                   </args>
                                   <wsdlFiles>
                                        <wsdlFile>${basedir}/src/main/resources/wsdl/jobservice.xml</wsdlFile>
                                        <wsdlFile>${basedir}/src/main/resources/wsdl/phoneService.xml</wsdlFile>
                                        <wsdlFile>${basedir}/src/main/resources/wsdl/languangeService.xml</wsdlFile>
                                        <wsdlFile>${basedir}/src/main/resources/wsdl/WeatherWS.xml</wsdlFile>
                                        <wsdlFile>${basedir}/src/main/resources/wsdl/MobileCodeWS.xml</wsdlFile>
                                   </wsdlFiles>
                              </configuration>
                              <id>wsimport-generate-egtestreportengine</id>
                              <phase>generate-sources</phase>
                         </execution>
                    </executions>
                    <dependencies>
                         <dependency>
                              <groupId>javax.xml</groupId>
                              <artifactId>webservices-api</artifactId>
                              <version>2.0.1</version>
                         </dependency>
                    </dependencies>
                    <configuration>
                         <sourceDestDir>${project.build.directory}/generated-sources/jaxws-wsimport</sourceDestDir>
                         <xnocompile>true</xnocompile>
                         <verbose>true</verbose>
                         <extension>true</extension>
                         <catalog>${basedir}/src/jax-ws-catalog.xml</catalog>
                         <target>2.0</target>
                    </configuration>
               </plugin>
